    nin x = xPixel / 30 - 2
    nin x0 = x
    nin y0 = y
    nin iter = 0
    foo (iter < 11 && x0 * x0 + y0 * y0 <= 4) {
      nin x1 = (x0 * x0) - (y0 * y0) + x
      nin y1 = 2 * x0 * y0 + y
      x0 = x1
      y0 = y1
      iter = iter + 1
    }
   A.seben(" .-:;+=xX$& "[iter])
